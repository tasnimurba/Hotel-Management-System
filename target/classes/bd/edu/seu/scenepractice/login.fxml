<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.scenepractice.controller.LoginController">
   <children>
      <Pane layoutX="-1.0" layoutY="-1.0" prefHeight="504.0" prefWidth="903.0" style="-fx-background-color: #CBD6E6;">
         <children>
            <TextField fx:id="numberSignField" layoutX="294.0" layoutY="182.0" prefHeight="32.0" prefWidth="383.0" promptText="Number" />
            <PasswordField fx:id="passSignField" layoutX="293.0" layoutY="236.0" prefHeight="32.0" prefWidth="388.0" promptText="Password" />
            <Button layoutX="429.0" layoutY="291.0" mnemonicParsing="false" onAction="#login" prefHeight="32.0" prefWidth="92.0" text="Login" />
            <Label layoutX="536.0" layoutY="83.0" text="Sign In">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <ImageView fitHeight="123.0" fitWidth="133.0" layoutX="414.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@5087579.png" />
               </image>
            </ImageView>
            <Button layoutX="426.0" layoutY="352.0" mnemonicParsing="false" onAction="#goForMembership" text="Go for membership" />
            <ImageView fitHeight="46.0" fitWidth="49.0" layoutX="458.0" layoutY="397.0" onMouseClicked="#home" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@10751558.png" />
